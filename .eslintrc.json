{
  "env": {
    "es2020": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "camelcase": "off",
    "max-len": ["error", { "code": 125 }]
    // "comma-dangle": "off",
    // "no-trailing-spaces": "off",
    // "object-curly-newline": "off",
    // "implicit-arrow-linebreak": "off"
  }
}
